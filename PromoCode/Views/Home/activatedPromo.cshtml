@using PromoCode.Models;


@model PagedList<PromoCode>
@{
}
<h3>@ViewData["Message"]</h3>
<p>Всего промокодов: @Model.Count</p>
 <form method="post" collor="#FFFFFF" >
@foreach(var s in Model)
    {
        <p>Имя: @s.name Дата выдачи:  @s.dataTimeCreated Дата активации:@s.activationDate </p>
    }
    
  

</form>

@if (Model.HasPreviousPage())
{
    <a asp-action="activatedPromo" asp-route-page="@(Model.CurrentPage-1)" asp-route-searchString="@ViewBag.Search">
        <button>
            назад
        </button>
    </a>
}
@if (Model.HasNextPage())
{
    <a asp-action="activatedPromo" asp-route-page="@(Model.CurrentPage+1)" asp-route-searchString="@ViewBag.Search">
        <button>
            вперед
        </button>
    </a>
}